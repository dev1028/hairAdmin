<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dbal.app.emp.mapper.MainMapper">

	<select id="getLoginInfo" resultType="emp">
		select *from employees where emp_email=#{emp_email} and
		emp_password=#{emp_password}
	</select>


<!--  -->
</mapper>
